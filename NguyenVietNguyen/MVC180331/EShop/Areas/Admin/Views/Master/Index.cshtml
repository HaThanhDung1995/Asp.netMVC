@using EShop.i18n
@model EShop.Models.Master
@{
    ViewBag.Title = @Global.MasterManager;
    ViewBag.Controller = "/Admin/Master";
}

<h2>@ViewBag.Title</h2>
<ul class="nav nav-tabs">
    <li class="active">
        <a data-toggle="tab" href="#edit"><span class="glyphicon glyphicon-edit"></span> @Global.Update</a>
    </li>
    <li>
        <a data-toggle="tab" href="#list"><span class="glyphicon glyphicon-list-alt"></span> @Global.List</a>
    </li>
</ul>

<div class="tab-content">
    <div id="edit" class="tab-pane fade in active">
        @Html.Partial("_Edit", Model)
    </div>
    <div id="list" class="tab-pane fade">
        @Html.Partial("_List")
    </div>
</div>


